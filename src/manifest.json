{
    "manifest_version": 2,
    "name": "Workspaces",
    "description": "Launch, navigate, and save workspaces",
    "version": "0.0.1",
    "icons": {
        "48": "icons/icon.png"
    },
    "permissions": ["tabs", "activeTab", "storage", "file://*/*", "https://*/*", "http://*/*"],
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Alt+A"
            },
            "description": "Opens the Workspaces popup"
        }
    },
    "browser_action": {
        "default_icon": {
            "19": "icons/icon.png"
        },
        "default_title": "Workspaces",
        "default_popup": "browser_action/index.html"
    }
}
